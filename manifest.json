{
	"name": "Website Info"
	, "description": "Display the website IP address and IP location."
	, "manifest_version": 2
	, "version": "0.1.3"
	, "background": {
		"scripts": ["background.js"]
	}
	, "permissions": ["http://*/*", "https://*/*", "tabs", "nativeMessaging", "webRequest"]
	, "content_scripts": [{
		"matches": ["http://*/*", "https://*/*"]
		, "js": ["main.js"]
		, "css": ["style.css"]
   }]
}